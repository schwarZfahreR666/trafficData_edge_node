<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.edge_node.mapper.UserMapper">
    <select id="queryUserByName" parameterType="string" resultType="com.example.edge_node.pojo.User" >
        select id,username,password,perms
        from user_table where username=#{username};
    </select>
</mapper>
